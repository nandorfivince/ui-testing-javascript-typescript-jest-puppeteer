{
  "name": "ui-testing-javascript-typescript-jest-puppeteer",
  "version": "1.0.0",
  "description": "It is a prototype project for experimental purposes using javascript/typescript/jest/puppeteer",
  "main": "index.js",
  "scripts": {
    "basic": "node node_modules/mocha/bin/mocha --timeout=30000 tests/basic",
    "e2e": "node node_modules/mocha/bin/mocha --timeout=30000 tests/e2e",
    "perf": "node node_modules/mocha/bin/mocha --timeout=30000 tests/performance",
    "test:snapshot": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nandorfivince/ui-testing-javascript-typescript-jest-puppeteer.git"
  },
  "author": "Nandorfi Vince",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nandorfivince/ui-testing-javascript-typescript-jest-puppeteer/issues"
  },
  "homepage": "https://github.com/nandorfivince/ui-testing-javascript-typescript-jest-puppeteer#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chrome-timeline": "0.0.12",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.11.1",
    "mocha": "^6.2.2",
    "puppeteer": "^2.0.0",
    "tracium": "^0.2.1"
  },
  "directories": {
    "test": "tests"
  }
}
